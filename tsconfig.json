{
    "compilerOptions": {
      "composite": true,
    "noEmit": true,
      "jsx": "react-jsx", 
      "module": "esnext",
      "target": "es6",
      "moduleResolution": "node",
      "allowJs": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }
  

